<div class="center">
  <%= link_to 'terug', :back %>
  <%= search_form_for @q, url: search_brands_path, html: {class: 'form-inline'}, method: :post do |f| %>
        <%= f.search_field :name_cont, placeholder: 'Search', class: 'form-control' %>
        <%= f.submit 'Submit', class: 'btn btn-default'%>
  <%end%>
</div>


<div id="articles" class="transitions-enabled">
    <% @brands.each do |brand|%>
    <div class="box panel panel-default">
       <%= link_to image_tag(brand.logo.url(:medium)), brand_path(brand.name)%></br>
    </div>
  <% end %>
  </div>
</div>







